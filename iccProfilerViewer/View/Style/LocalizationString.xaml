<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <!--Main menu-->
    <system:String x:Key="MenuItem_Header_File">Файл</system:String>
    <system:String x:Key="MenuItem_Open">Открыть...</system:String>
    <system:String x:Key="MenuItem_Exit">Выход</system:String>
    <!--Main menu-->
    
    <!--Label-->
    <system:String x:Key="Label_CmmType">Тип СММ</system:String>
    <system:String x:Key="Label_ProfileVersion">Версия профиля</system:String>
